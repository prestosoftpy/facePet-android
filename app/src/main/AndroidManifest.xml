<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="py.com.prestosoftware.facepet">

    <!-- permiso para salida de internet -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".FacePetApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.main.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.users.login.LoginActivity" />
        <activity android:name=".ui.users.register.RegisterActivity" />
        <activity android:name=".ui.users.profile.ProfileActivity" />
        <activity android:name=".ui.reservations.Register.RegisterReservationsActivity" />
        <activity android:name=".ui.Donations.DonationsActivity" />
    </application>

</manifest>